#include<stdio.h>
#include<windows.h>
int main()
{
    while(1)
    {
        int x=25,y=10;
        for(;y<20;y++)
        {
            system("cls");
            for(int i=1;i<y;i++)
                printf("\n");
            for(int i=1;i<x;i++)
                printf(" ");
            printf("O\n");
            Sleep(20);
        }

        for(;y>10;y--)
        {
            system("cls");
            for(int i=1;i<y;i++)
                printf("\n");
            for(int i=1;i<x;i++)
                printf(" ");
            printf("O\n");
            Sleep(20);
        }
    }

}
